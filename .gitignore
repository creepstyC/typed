# Ignorar la carpeta node_modules
/node_modules
/frontend/node_modules
/backend/node_modules

# Ignorar archivos de dependencias de npm o yarn
/package-lock.json
/yarn.lock

# Ignorar los archivos de configuración de entorno
.env
/frontend/.env
/backend/.env

# Ignorar logs
*.log
/frontend/*.log
/backend/*.log

# Ignorar las carpetas de compilación y distribución
/frontend/build
/frontend/dist
/backend/build
/backend/dist

# Ignorar archivos de dependencias de compilación (si usas un bundler como webpack)
frontend/.cache
frontend/.parcel-cache

# Ignorar archivos específicos de OS
.DS_Store

# Ignorar archivos generados por IDEs y editores (Ejemplo: VS Code, JetBrains)
.vscode/
.idea/
*.swp
*.swo

# Ignorar dependencias de contenedores (si usas Docker)
frontend/docker/
backend/docker/

# Ignorar cualquier otro archivo o carpeta específica
